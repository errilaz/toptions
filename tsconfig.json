{
  "compilerOptions": {
    "outDir": "lib",
    "target": "es2018",
    "module": "commonjs",
    "lib": ["ES2020"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "strict": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false,
  },
  "files": ["src/topt.ts"]
}